import { createSelector } from 'reselect';

const node = (state) => state.get('{{camelCase moduleName}}');
{{#each stateJSON}}
{{#if isImmutable}}
const get{{properCase @key}} = () => createSelector(node, (s) => s.get('{{@key}}'));
{{else}}
const get{{properCase @key}} = () => createSelector(node, (s) => s.{{@key}});
{{/if}}
{{/each}}

export {
  {{#each stateJSON}}
  get{{properCase @key}},
  {{/each}}
};
